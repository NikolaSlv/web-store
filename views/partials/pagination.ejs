<nav class="pt-5" aria-label="Page navigation">
    <ul class="pagination justify-content-center">
    <li class="page-item">
        <a class="page-link" href="#" onclick="pagination(-1, '<%= maxPage %>', -1)" aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
        </a>
    </li>
    <% for (i = 1; i <= maxPage; i++) { %>
        <li class="page-item">
            <a id="page-<%= i %>" class="page-link" href="#" 
            onclick="pagination(-1, '<%= maxPage %>', `<%= i %>`)"><%= i %></a>
        </li>
    <% } %>
    <li class="page-item">
        <a class="page-link" href="#" onclick="pagination(1, '<%= maxPage %>', -1)" aria-label="Next">
            <span aria-hidden="true">&raquo;</span>
        </a>
    </li>
    </ul>
</nav>
<script>
    document.getElementById("page-<%= page %>").classList.add("active");
</script>